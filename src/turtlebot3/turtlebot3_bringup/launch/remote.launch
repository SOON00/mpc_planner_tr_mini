<launch>
  <!-- Arguments -->
  <arg name="multi_robot_name" default="" doc="prefix for multi robot setup" />

  <!-- Load robot_description from static URDF file -->
  <param name="robot_description" textfile="$(find tracer_mini)/urdf/tracer_mini.urdf" />

  <!-- Robot State Publisher -->
  <node pkg="robot_state_publisher" type="robot_state_publisher" name="robot_state_publisher">
    <param name="publish_frequency" type="double" value="50.0" />
    <param name="tf_prefix" value="$(arg multi_robot_name)" />
  </node>
</launch>
